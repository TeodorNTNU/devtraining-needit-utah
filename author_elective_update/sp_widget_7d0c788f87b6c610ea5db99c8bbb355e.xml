        <name>Widget NeedIt</name>
        <option_schema/>
        <public>false</public>
        <roles/>
        <script><![CDATA[(function() {
    var gr = new GlideRecord('x_58872_needit_needit_records_import');
    gr.query();
    var records = [];
    while (gr.next()) {
        records.push({
            state: gr.getValue('state'),
            requestType: gr.getValue('request_type'),
            requestedFor: gr.getValue('requested_for'),
            target: gr.getValue('target'),
            predCost: gr.getValue('pred_cost'),
            predHours: gr.getValue('pred_hours')
        });
    }
    data.records = records;
})();
]]></script>
